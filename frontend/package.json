{
  "name": "jak-surveillance",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prebuild": "node build-date.cjs",
    "dev": " npm i &vite --port 8080 --open --host",
    "dev2": " npm i & vite --port 8080",
    "devBrutal": "  vite --port 8080 --open -d --force --host",
    "devDebug": "  vite --port 8080 --open -d --host",
    "build": "npm i & tsc && vite build --outDir ../backend/jaksec --emptyOutDir --base /",
    "buildWatch": "tsc --watch & vite build --outDir ../backend/jaksec --emptyOutDir --base / --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "all": " npm i & tsc --watch & vite build --outDir ../backend/jaksec --emptyOutDir  --base / --watch",
    "alldev": "npm i &  vite --port 8080",
    "start": "npm run dev",
    "docs": "typedoc src",
    "docs:watch": "nodemon --watch 'src/**/*.ts*' --exec npm run docs"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@yudiel/react-qr-scanner": "^1.2.10",
    "chart.js": "^4.4.1",
    "date-fns": "^3.3.1",
    "docdash": "^2.0.2",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.3.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "react": "^18.2.0",
    "react-calendar": "^4.8.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-qr-code": "^2.0.12",
    "react-router-dom": "^6.22.1",
    "react-toastify": "^10.0.4",
    "socket.io-client": "^4.7.4",
    "sort-by": "^1.2.0",
    "typedoc-material-theme": "^1.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdoc": "^4.0.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.27.2",
    "typedoc": "^0.26.10",
    "typedoc-comments": "^1.0.1",
    "typedoc-plugin-coverage": "^3.4.1",
    "typedoc-plugin-extras": "^3.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-pwa": "^0.19.0"
  }
}
