{
	"name": "jak-surveillance",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --port 8080 --open --host",
		"dev2": "vite --port 8080",
		"devBrutal": "vite --port 8080 --open -d --force --host",
		"devDebug": "vite --port 8080 --open -d --host",
		"build": "tsc && vite build --outDir ../backend/jaksec --minify --emptyOutDir --base /",
		"buildWatch": "tsc --watch & vite build --outDir ../backend/jaksec --emptyOutDir --minify  --base / --watch",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"all": " npm i & tsc --watch & vite build --outDir ../backend/jaksec --emptyOutDir --minify  --base / --watch",
		"alldev": "npm i & vite --port 8080"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.4.3",
		"@tanstack/react-router": "^0.0.1-beta.198",
		"date-fns": "^2.30.0",
		"html5-qrcode": "^2.3.8",
		"localforage": "^1.10.0",
		"match-sorter": "^6.3.1",
		"react": "^18.2.0",
		"react-calendar": "^4.6.1",
		"react-dom": "^18.2.0",
		"react-qr-code": "^2.0.12",
		"react-router-dom": "^6.17.0",
		"socket.io-client": "^4.7.2",
		"sort-by": "^1.2.0"
	},
	"devDependencies": {
		"@tanstack/eslint-plugin-query": "^5.0.5",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.45.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"postcss": "^8.4.31",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.0.2",
		"vite": "^4.4.5",
		"vite-plugin-pwa": "^0.16.5"
	}
}
