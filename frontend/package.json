{
  "name": "attendanceappfrontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prebuild": "node build-date.cjs",
    "dev": "vite --port 8080 --host",
    "dev:open": "vite --port 8080 --open",
    "dev:force": "vite --port 8080 --open --force --host --debug",
    "build": "tsc && vite build --outDir ../backend/jaksec --emptyOutDir --base /",
    "build:watch": "concurrently \"tsc --watch\" \"vite build --watch --outDir ../backend/jaksec --emptyOutDir --base /\"",
    "dev:watch": "concurrently \"tsc --watch\" \"vite --port 8080 --host\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "docs": "typedoc src",
    "docs:watch": "nodemon --watch 'src/**/*.ts*' --exec npm run docs"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@headlessui/react": "^2.2.0",
    "@mui/icons-material": "^6.4.3",
    "@mui/material": "^6.4.3",
    "@tailwindcss/vite": "^4.0.4",
    "@yudiel/react-qr-scanner": "^2.2.1",
    "chart.js": "^4.4.7",
    "country-flag-icons": "^1.5.16",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "docdash": "^2.0.2",
    "framer-motion": "^12.4.2",
    "i18next": "^24.2.2",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "localforage": "^1.10.0",
    "match-sorter": "^8.0.0",
    "react": "^19.0.0",
    "react-calendar": "^5.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "react-qr-code": "^2.0.15",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^7.1.5",
    "react-toastify": "^11.0.3",
    "recharts": "^2.15.1",
    "socket.io-client": "^4.8.1",
    "sort-by": "^1.2.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.4",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.0",
    "eslint-plugin-import": "^2.31.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.0.4",
    "terser": "^5.38.1",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-pwa": "^0.21.1",
    "concurrently": "^8.2.2"
  }
}
