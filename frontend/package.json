{
	"name": "jak-surveillance",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"prebuild": "node build-date.cjs",
		"dev": " vite --port 8080 --open --host",
		"dev2": "  vite --port 8080",
		"devBrutal": "  vite --port 8080 --open -d --force --host",
		"devDebug": "  vite --port 8080 --open -d --host",
		"build": "npm i & tsc && vite build --outDir ../backend/jaksec --emptyOutDir --base /",
		"buildWatch": "tsc --watch & vite build --outDir ../backend/jaksec --emptyOutDir --base / --watch",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"all": " npm i & tsc --watch & vite build --outDir ../backend/jaksec --emptyOutDir  --base / --watch",
		"alldev": "npm i &  vite --port 8080",
		"docs": "typedoc src",
		"docs:watch": "nodemon --watch 'src/**/*.ts*' --exec npm run docs"
	},
	"dependencies": {
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@mui/icons-material": "^6.1.5",
		"@mui/material": "^6.1.5",
		"@yudiel/react-qr-scanner": "^2.0.8",
		"chart.js": "^4.4.5",
		"date-fns": "^4.1.0",
		"docdash": "^2.0.2",
		"dotenv": "^16.4.5",
		"dotenv-cli": "^7.4.2",
		"jspdf": "^2.5.2",
		"jspdf-autotable": "^3.8.4",
		"localforage": "^1.10.0",
		"match-sorter": "^7.0.0",
		"react": "^18.3.1",
		"react-calendar": "^5.1.0",
		"react-chartjs-2": "^5.2.0",
		"react-dom": "^18.3.1",
		"react-qr-code": "^2.0.15",
		"react-router-dom": "^6.27.0",
		"react-toastify": "^10.0.6",
		"socket.io-client": "^4.8.0",
		"sort-by": "^1.2.0",
		"typedoc-material-theme": "^1.1.0",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/eslint-plugin": "^8.11.0",
		"@typescript-eslint/parser": "^8.11.0",
		"@vitejs/plugin-react-swc": "^3.7.1",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"eslint": "^9.13.0",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.14",
		"jsdoc": "^4.0.4",
		"postcss": "^8.4.47",
		"tailwindcss": "^3.4.14",
		"terser": "^5.36.0",
		"typedoc": "^0.26.10",
		"typedoc-comments": "^1.0.1",
		"typedoc-plugin-coverage": "^3.3.0",
		"typedoc-plugin-extras": "^3.1.0",
		"typescript": "^5.6.3",
		"vite": "^5.4.10",
		"vite-plugin-pwa": "^0.20.5"
	}
}
