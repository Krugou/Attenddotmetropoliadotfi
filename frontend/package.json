{
	"name": "jak-surveillance",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"prebuild": "node build-date.cjs",
		"dev": " vite --port 8080 --open --host",
		"dev2": "  vite --port 8080",
		"devBrutal": "  vite --port 8080 --open -d --force --host",
		"devDebug": "  vite --port 8080 --open -d --host",
		"build": "tsc && vite build --outDir ../backend/jaksec --minify --emptyOutDir --base /",
		"buildWatch": "tsc --watch & vite build --outDir ../backend/jaksec --emptyOutDir --minify  --base / --watch",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"all": " npm i & tsc --watch & vite build --outDir ../backend/jaksec --emptyOutDir --minify  --base / --watch",
		"alldev": "npm i &  vite --port 8080"
	},
	"dependencies": {
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.14.18",
		"@mui/material": "^5.14.18",
		"@yudiel/react-qr-scanner": "^1.2.2",
		"date-fns": "^2.30.0",
		"dotenv": "^16.3.1",
		"dotenv-cli": "^7.3.0",
		"localforage": "^1.10.0",
		"match-sorter": "^6.3.1",
		"react": "^18.2.0",
		"react-calendar": "^4.6.1",
		"react-dom": "^18.2.0",
		"react-qr-code": "^2.0.12",
		"react-router-dom": "^6.17.0",
		"react-toastify": "^9.1.3",
		"socket.io-client": "^4.7.2",
		"sort-by": "^1.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"autoprefixer": "^10.4.16",
		"cross-env": "^7.0.3",
		"eslint": "^8.45.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"postcss": "^8.4.31",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.0.2",
		"vite": "^5.0.0",
		"vite-plugin-pwa": "^0.17.0"
	}
}
